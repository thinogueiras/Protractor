    "eslint-plugin-import": "^2.25.4",
    "protractor": "^7.0.0",
    "protractor-helper": "^4.2.1",
    "standardjs": "^1.0.0-alpha"
  },
  "dependencies": {
    "@faker-js/faker": "^6.0.0"
  }
